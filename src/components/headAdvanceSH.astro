<!-- cardloanがURLに含まれない場合js_service_organic_cookie_write.jsを読み込む -->
<script>
document.addEventListener('DOMContentLoaded',() => {
	const chkUrl = new URL(location.href);
	if (!document.location.href.includes("/cardloan/") || !chkUrl.pathname.startsWith("/cardloan/"))
	{
		const newScript = document.createElement('script');
		newScript.src = "/js/js_service_organic_cookie_write.js";
		newScript.defer = true;
		document.head.appendChild(newScript);
	}
})
</script>
<!-- //cardloanがURLに含まれない場合js_service_organic_cookie_write.jsを読み込む -->

